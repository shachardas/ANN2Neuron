   "source": [
    "from dish import Dish\n",
    "import math\n",
    "\n",
    "dish = Dish()\n",
    "\n",
    "node = dish.node()\n",
    "node2 = dish.node()\n",
    "\n",
    "dish.stimulateNode(node, lambda x : math.sin(0.1*x))\n",
    "#dish.stimulateNode(node)\n",
    "dish.connect(node, node2, 1)\n",
    "dish.monitorNode(node2)\n",
    "\n",
    "dish.record(3)\n"
   ]
